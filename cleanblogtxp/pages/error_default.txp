<txp:output_form form="clean_nav" />

    <!-- Page Header -->

    <header class="masthead" style="background-image: url('<txp:site_url />assets/img/error-bg.jpg')">

      <div class="overlay"></div>
      <div class="container">
        <div class="row">
          <div class="col-lg-8 col-md-10 mx-auto">
